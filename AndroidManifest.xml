<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.etechclub.working_with_activities">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".Menu">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SubActivity1">
            <intent-filter>
                <action android:name="android.intent.action.SUBACTIVITY1" />

                <category android:name="com.etechclub.working_with_activities.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>
